# last modified 11/30/1999

# No need to read this file if noninteractive

if (! $?prompt) exit		# prompt is set if interactive.
if (! $?term) exit		# term is set if we have a tty attached (needed for at)

# If there's any mail, show who it's from, and add a cr

from
echo ""

# Load the standard .cshrc file first

source /usr/local/environment/.cshrc

# Set custom settings for tcsh

set prompt="%m:/%// [%h] "
set history=256

# Set up aliases

alias lsl ls -al
alias ls ls -CF
